PROGRAM ERROR3;
VAR I:INTEGER;  
     N:INTEGER;  
     P:INTEGER;  
     Q:INTEGER;
     C:CHAR;

PROCEDURE HANOI(N:INTEGER; S:INTEGER; Z:INTEGER);
BEGIN
  IF N != 0 THEN
    BEGIN
      I:=I+1;  
      CALL WRITELN;
    END
END;

BEGIN
  P:=1;  
  Q:=2;
  I:=0;  
  CALL HANOI(N, P);  (* LỖI: HANOI cần 3 tham số nhưng chỉ truyền 2 *)
END.
