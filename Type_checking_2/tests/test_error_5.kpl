PROGRAM ERROR5;
   CONST C1 = 10;
  	 C2 = 'a';
   TYPE T1 = ARRAY(. 10 .) OF INTEGER;
   VAR V1 : INTEGER;
       V2 : ARRAY(. 10 .) OF T1;

   FUNCTION F(P1 : INTEGER; P2 : CHAR) : INTEGER;
     BEGIN
       F := C1;
     END;

   PROCEDURE P(V1 : INTEGER);
     CONST C1 = 'a';
           C3 = 10;
     TYPE T1 = INTEGER;
          T2 = ARRAY(. 10 .) OF T1;
     VAR V2 : T2;
         V3 : CHAR;
     BEGIN
	V3 := 'a';
        V1 := F(C3, 10);  (* LỖI: Tham số thứ 2 cần CHAR nhưng truyền INTEGER *)
     END;

BEGIN
     CALL P(C1);
END.
